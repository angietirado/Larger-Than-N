/**
 *
 * @author Angie Tirado
 */
import java.util.Scanner;
public class LargerThanN {

    public static void main(String[] args) {
       int num = 0;
       int[] numbers = {5, 6, 9, 11, 22, 54, 73};
       
       for(int i = 0; i < numbers.length; i++){
           greaterThanN(numbers, num);
       }
        
       
       
    }
    
    public static void greaterThanN(int[] array, int n){
        
        Scanner userInput = new Scanner(System.in);
        
        System.out.println("Enter a number: ");
        
        n = userInput.nextInt();
        
        //int greaterThanN = array[0];
        
        for(int i = 0; i < array.length; i++){
            if(array[i] > n){
                System.out.println("This number " + array[i] + " is greater than " + n);
                
            }
            else{
                System.out.println("There is no number that is greater than " + n);
            }
        }
    }
}

